<?xml version="1.0"?>
<configuration>
  <system.serviceModel>
	<services>
   <service name="multidbapi.ServiceModel">
     <endpoint address="" binding="webHttpBinding" contract="multidbapi.ServiceInterface" />
     <host>
       <baseAddresses>
         <add baseAddress="http://localhost:8733/ServiceAPI/" />
       </baseAddresses>
     </host>
   </service>
 </services>
	<behaviors>
	  <endpointBehaviors>
		  <behavior>
		    <webHttp/>
		  </behavior>
	  </endpointBehaviors>
    <serviceBehaviors>
      <behavior>
        <serviceMetadata httpGetEnabled="True" httpsGetEnabled="True"/>
        <serviceDebug includeExceptionDetailInFaults="True"/>
      </behavior>
    </serviceBehaviors>
	</behaviors>
  </system.serviceModel>
  <startup>
	<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
</configuration>
