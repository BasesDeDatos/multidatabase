<?xml version="1.0"?>
<configuration>
  <system.serviceModel>
	<services>
	  <service name="multidbapi.Service1">
      <host>
        <baseAddresses>
          <add baseAddress = "http://localhost:8733/Service1/" />
        </baseAddresses>
      </host>
		  <endpoint address="" 
				    binding="webHttpBinding" 
				    contract="multidbapi.IService1"/>
	  </service>
	</services>
	<behaviors>
	  <endpointBehaviors>
		  <behavior>
		    <webHttp />
		  </behavior>
	  </endpointBehaviors>
    <serviceBehaviors>
      <behavior>
        <serviceMetadata httpGetEnabled="True" httpsGetEnabled="True"/>
        <serviceDebug includeExceptionDetailInFaults="True" />
      </behavior>
    </serviceBehaviors>
	</behaviors>
  </system.serviceModel>
  <startup>
	<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
</configuration>